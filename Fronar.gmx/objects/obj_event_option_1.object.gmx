            <string>///Do the event choice
//NOTE YOU MUST CHECK PART2 SO THAT PLAYERS CANT SPAM CLICK FOR MORE POWER
if (!global.event_part2){
    global.event_selected_option = 0;
    global.event_part2 = true;
    switch(global.event_id){
        case 0:         
            //Abadoned Shack bayonet powerup +3 melee
            //SWITCH THIS TO MELEE WHEN MELEE IMPLEMENTED
            ds_list_replace(ds_list_find_value(global.eventCharList, charId), 11, ds_list_find_value(ds_list_find_value(global.charList, charId), 11) + 3);      
            break;
        case 1:
            //Spike Pit
            //Nothing Happens
            break;
        case 2:
            //Wagon Lady
            modifyFood(-10);
            modifyParts(5);       
            break;
        case 3:
            //Box of Heroin
            //+1 Movespeed
            ds_list_replace(ds_list_find_value(global.eventCharList, charId), 10,ds_list_find_value(ds_list_find_value(global.eventCharList, charId), 10) + 1);
            break;
        case 4:
            //Lightning
            subevent = irandom(2);
            if(subevent &lt;= 1) {
                //2/3rds chance
                //+1 Crew Attack Speed
                global.event_subevent = 0;
                var i;
                for(i = 0; i &lt; ds_list_size(global.eventCharList); ++i){
                    ds_list_replace(ds_list_find_value(global.eventCharList, i), 12 ,ds_list_find_value(ds_list_find_value(global.eventCharList, i), 12) + 1);      
                }
            } else {
                //1/3rd chance
                //-1 Character Attack Speed
                global.event_subevent = 1;
                ds_list_replace(ds_list_find_value(global.eventCharList, 0), 12 ,ds_list_find_value(ds_list_find_value(global.eventCharList, 0), 12) - 1);      
            }
            break;
        case 5:
            //Circus
            modifyMedicine(5);
            break;
        case 6:
            //Forgotten Weapon
            //Set health to 0.  This kills the character.
            ds_list_replace(ds_list_find_value(global.eventCharList, 0), 7, 0);      
            break;
        case 8:
            //Library
            ds_list_replace(ds_list_find_value(global.eventCharList, charId), 11 ,ds_list_find_value(ds_list_find_value(global.eventCharList, charId), 11) + 5);      
            break;
    }
}
