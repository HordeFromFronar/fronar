          <argument>
            <kind>1</kind>
            <string>//NOTE YOU MUST CHECK PART2 SO THAT PLAYERS CANT SPAM CLICK FOR MORE POWER
if (!global.event_part2){
    global.event_selected_option = 1;
    global.event_part2 = true;
    switch(global.event_id){
        case 0:         
            //Abadoned Shack bayonet powerup +3 melee
            //SWITCH THIS TO MELEE WHEN MELEE IMPLEMENTED
            ds_list_replace(ds_list_find_value(global.eventCharList, charId), 11, ds_list_find_value(ds_list_find_value(global.eventCharList, charId), 11) + 3);      
            break;
        case 1:
            //Spike Pit
            subevent = irandom(4);
            if (subevent == 0) {
                global.event_subevent = 0;
                //+10 Food
                ds_list_replace(global.resourceList, 2, ds_list_find_value(global.resourceList, 2) + 10);
                //-1 Health
                ds_list_replace(ds_list_find_value(global.eventCharList, charId), 7, ds_list_find_value(ds_list_find_value(global.eventCharList, charId), 7) - 1);      
                //Apply a slowdown
                ds_list_replace(ds_list_find_value(global.eventCharList, charId), 10, 0.5);
            } else if (subevent == 1) {
                global.event_subevent = 1;
                //-1 Health
                ds_list_replace(ds_list_find_value(global.eventCharList, charId), 7, ds_list_find_value(ds_list_find_value(global.eventCharList, charId), 7) - 1);      
                //Apply a slowdown
                ds_list_replace(ds_list_find_value(global.eventCharList, charId), 10, 0.5);
            } else if (subevent &lt; 4) {
                global.event_subevent = 2;
                //+10 Food
                ds_list_replace(global.resourceList, 2, ds_list_find_value(global.resourceList, 2) + 10);
            } else {
                global.event_subevent = 3;
                //Nothing happens
            }
            break;
        case 2:
            //Wagon Lady
            modifyFood(-10);
            modifyMedicine(5);
            break;
        case 3:
            //Box of Heroin
            //-30 Health
            ds_list_replace(ds_list_find_value(global.eventCharList, charId), 7 ,ds_list_find_value(ds_list_find_value(global.eventCharList, charId), 7) - 30);      
            //+2 Movespeed
            ds_list_replace(ds_list_find_value(global.eventCharList, charId), 10,ds_list_find_value(ds_list_find_value(global.eventCharList, charId), 10) + 2);
            break;
        case 4:
            //Lightning
            //Nothing happens
            break;
        case 5:
            //Circus
            modifyFood(12);
            break;
        case 6:
            //Forgotten Weapon
            //Set health to 0.  This kills the character.
            ds_list_replace(ds_list_find_value(global.charList, 0), 7, 0);      
            break;
        case 8:
            //Library
            //+10 to all health
            var i;
            for(i = 0; i &lt; ds_list_size(global.eventCharList); ++i){
                healevent(i, 10);
            }
            break;
            
    }
}
